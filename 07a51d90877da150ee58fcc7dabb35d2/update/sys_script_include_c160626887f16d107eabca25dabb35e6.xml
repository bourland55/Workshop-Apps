        <name>deficiencyUtils</name>
        <script><![CDATA[var deficiencyUtils = Class.create();
deficiencyUtils.prototype = {
    initialize: function() {
    },
getKeyOps: function(myIcn) {
	
	var myKeyOps = [];
	
	var gr = new GlideRecord('x_snc_deficiency_0_icn_keyop_combined');
	gr.addQuery('icn', myIcn);
	gr.query();
	
	while(gr.next()){

		myKeyOps.push(gr.keyop.toString());
		
	}
	
		return myKeyOps;
	},
    type: 'deficiencyUtils'
};]]></script>
